  "scripts": {
    "dev": "nodemon",
    "start": "mocha -r ./test/helpers/initializeMocha.ts -O outputDir=test/reports,testDir=test/unit,fileName=testOutput,reporter=spec --reporter ./dist",
    "lint": "eslint .",
    "build": "babel src --extensions \".ts\"  --out-dir dist",
    "build-and-run": "npm run build && npm run start",
    "test:unit": "mocha -r ./test/helpers/initializeMocha.ts",
    "test:browser": "karma start karma.conf.js"
  },
  "nodemonConfig": {
    "ignore": ["test/**/*", "dist/**/*"],
    "exec": "npm run build-and-run"
  }
